    "#function for reverse string\n",
    "def string_reverse(str1):\n",
    "    rev_str=\"\"\n",
    "    for i in str1:\n",
    "        rev_str=i+rev_str\n",
    "    return rev_str\n",
    "\n",
    "#get string from user \n",
    "str1=input(\"Enter a string:\")\n",
    "#call the function\n",
    "reverse_string=string_reverse(str1)\n",
    "print(reverse_string)"
   ]
